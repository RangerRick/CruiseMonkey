{
  "name": "CruiseMonkey",
  "version": "2016.1.1",
  "minSdk": "14",
  "description": "Keep Track of Your Cruise",
  "build": 40407,
  "dependencies": {
    "feedparser": "*",
    "googleapis": "*",
    "moment": "*",
    "request": "*"
  },
  "devDependencies": {
    "assert-plus": "^1.0.0",
    "bower": "*",
    "commonmark": "*",
    "cordova-android": "*",
    "cordova-config-utils": "git+https://github.com/RangerRick/cordova-config-utils.git#master",
    "css-loader": "^0.23.1",
    "eslint": "^3.7.1",
    "eslint-friendly-formatter": "^1.2.2",
    "eslint-loader": "^1.3.0",
    "exports-loader": "^0.6.3",
    "expose-loader": "^0.7.1",
    "file-loader": "^0.8.5",
    "fs.extra": "*",
    "ghooks": "^1.0.3",
    "html-loader": "^0.4.2",
    "imports-loader": "^0.6.5",
    "jsdom": "^8.0.3",
    "jshint": "*",
    "jshint-stylish": "*",
    "line-reader": "*",
    "lodash": "^4.5.0",
    "mocha": "^2.4.5",
    "moment": "^2.11.2",
    "moment-timezone": "^0.5.0",
    "ng-annotate-webpack-plugin": "^0.1.2",
    "ngtemplate-loader": "^1.3.1",
    "node-sass": "^3.4.2",
    "phantomjs": "^2.1.3",
    "phantomjs-prebuilt": "^2.1.3",
    "sass-loader": "^3.1.2",
    "script-loader": "^0.6.1",
    "shelljs": "*",
    "string-hash": "^1.1.0",
    "style-loader": "^0.13.0",
    "twitter-text": "^1.13.3",
    "url-loader": "^0.5.7",
    "uuid": "^2.0.1",
    "webpack": "^1.12.12",
    "yargs": "^3.32.0"
  },
  "cordovaPlugins": [
    "cordova-plugin-console",
    "cordova-plugin-device",
    "cordova-plugin-file",
    "cordova-plugin-file-transfer",
    "cordova-plugin-network-information",
    "cordova-plugin-dialogs",
    "cordova-plugin-splashscreen",
    "cordova-plugin-statusbar",
    "cordova-plugin-whitelist",
    "ionic-plugin-keyboard",
    "cordova-plugin-camera",
    "https://github.com/zfoltin/cordova-plugin-app-event.git",
    "cordova-plugin-inappbrowser",
    "cordova-plugin-spinner-dialog",
    "https://github.com/katzer/cordova-plugin-local-notifications.git",
    "https://github.com/OpenNMS/cordova-plugin-json-storage.git",
    "https://github.com/RangerRick/cordova-certificate-plugin.git",
    "https://github.com/RangerRick/cordova-HTTP.git",
    "https://github.com/Red-Folder/bgs-core.git",
    "https://github.com/VersoSolutions/CordovaClipboard.git",
    "cordova-plugin-datepicker",
    "cordova-plugin-filepath",
    "cordova-plugin-chrome-apps-system-memory",
    "https://github.com/RangerRick/cordova-HTTP.git",
    "https://github.com/RangerRick/photoviewer.git#master",
    "cordova-plugin-crosswalk-webview",
    "https://github.com/christocracy/cordova-plugin-background-fetch.git",
    "https://github.com/RangerRick/cordova-plugin-simple-image-picker.git",
    "https://github.com/EddyVerbruggen/Toast-PhoneGap-Plugin.git"
  ],
  "cordovaPlatforms": [
    {
      "platform": "browser",
      "version": "4.0.0",
      "locator": "browser"
    },
    {
      "platform": "ios",
      "version": "3.9.2",
      "locator": "ios"
    },
    {
      "platform": "android",
      "version": "5.1.0",
      "locator": "android"
    }
  ],
  "config": {
    "ghooks": {
      "pre-push": "npm run test",
      "post-merge": "npm install",
      "post-rewrite": "npm install"
    }
  },
  "scripts": {
    "setup": "bower install && npm install && ionic state reset",
    "build": "webpack --colors --display-error-details && ionic build",
    "prepare": "webpack --colors --display-error-details && ionic prepare",
    "increment": "bin/update-build-number.js",
    "release": "npm run lint && npm run test && webpack --colors --display-error-details --env=production && ionic build --release && bin/sign-android.sh",
    "verbose-build": "npm run lint && npm run test && webpack --debug --colors --display-modules --display-chunks --display-error-details --display-origins --display-reasons",
    "genymotion": "ionic run -slc android",
    "android": "ionic emulate -slc android",
    "ios": "ionic emulate -slc --address=localhost ios",
    "device": "ionic run --device -slc ios",
    "watch": "webpack --colors --display-error-details --watch",
    "lint": "eslint . --ext .js",
    "test": "mocha test"
  }
}
