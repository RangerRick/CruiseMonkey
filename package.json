  "scripts": {
    "setup": "bower install && npm install && ionic state reset",
    "pack": "webpack --colors --display-error-details",
    "build": "npm run pack -- --mode=development --optimize-minimize=false",
    "build-release": "npm run pack -- --mode=production --optimize-minimize=true",
    "prepare": "npm run build && npx cap sync",
    "increment": "bin/update-build-number.js && npm run prepare",
    "release": "npm run lint && npm run test && npm run build-release && npx cap copy",
    "verbose-build": "npm run lint && npm run test && npm run build -- --display-modules --display-chunks --display-error-details --display-origins --display-reasons",
    "genymotion": "ionic cordova run -slc android",
    "android": "ionic cordova emulate -slc android",
    "ios": "ionic cordova emulate -slc --address=localhost ios --target=iPhone-X",
    "device": "ionic cordova run --device -slc ios",
    "watch": "webpack --colors --display-error-details --watch",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -u",
    "lint": "eslint lib --ext .js --ext .ts",
    "test": "mocha test"
