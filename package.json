  "config": {
    "ghooks": {
      "pre-push": "npm run test"
    }
  },
  "jest": {
    "moduleNameMapper": {
      ".*\\.png$": "<rootDir>/test/dummy.js"
    },
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "testEnvironment": "jest-environment-jsdom-sixteen",
    "testMatch": [
      "**/test/**/*.spec.js",
      "**/test/**/*.spec.ts"
    ],
    "transformIgnorePatterns": [
      "<rootDir>/bower_components/",
      "<rootDir>/node_modules/(?!(capacitor-ibeacon)/)"
    ]
  },
  "scripts": {
    "setup": "bower install && npm --pure-lockfile --prefer-offline --no-progress install && npx cap sync",
    "postinstall": "jetifier",
    "pack": "webpack --colors --display-error-details",
    "build": "npm run pack -- --mode=development --optimize-minimize=false",
    "build-release": "npm run pack -- --mode=production --optimize-minimize=true",
    "release": "npm run test && npm run build-release && npx cap sync",
    "prepare-tree": "npm run build && npx cap sync",
    "increment": "bin/update-build-number.js && npm run prepare-tree",
    "verbose-build": "npm run lint && npm run test && npm run build -- --display-modules --display-chunks --display-error-details --display-origins --display-reasons",
    "genymotion": "ionic cap run --consolelogs --livereload --external android",
    "android": "ionic cap run --consolelogs --livereload --external android",
    "ios": "ionic cap run --consolelogs --livereload --external ios",
    "device": "ionic cap run --device --consolelogs --livereload --external ios",
    "watch": "webpack --colors --display-error-details --watch",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -u",
    "lint": "eslint lib --ext .js --ext .ts",
    "test": "npm run lint && jest",
    "watch-test": "npm run build && jest --watch",
    "resources": "cordova-res ios && cordova-res android && node resources/generate-resources.js"
  }
}
