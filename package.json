    {
      "platform": "browser",
      "version": "4.0.0",
      "locator": "browser"
    },
    {
      "platform": "ios",
      "version": "3.9.2",
      "locator": "ios"
    },
    {
      "platform": "android",
      "version": "5.0.0",
      "locator": "android"
    }
  ],
  "scripts": {
    "build": "webpack --colors --display-error-details && ionic build",
    "prepare": "webpack --colors --display-error-details && ionic prepare",
    "increment": "bin/update-build-number.js",
    "release": "webpack --colors --display-error-details --env=production && ionic build --release && bin/sign-android.sh",
    "verbose-build": "webpack --debug --colors --display-modules --display-chunks --display-error-details --display-origins --display-reasons",
    "genymotion": "ionic run -slc android",
    "android": "ionic emulate -slc android",
    "ios": "ionic emulate -slc --address=localhost ios",
    "device": "ionic run --device -slc ios",
    "watch": "webpack --colors --display-error-details --watch",
    "lint": "eslint . --ext .js"
  }
