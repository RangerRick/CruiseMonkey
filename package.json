    "uuid": "^3.0.1",
    "webpack": "1.12.15",
    "yargs": "^6.6.0"
  },
  "cordovaPlugins": [
    "cordova-plugin-console",
    "cordova-plugin-device",
    "cordova-plugin-file",
    "cordova-plugin-file-transfer",
    "cordova-plugin-network-information",
    "cordova-plugin-dialogs",
    "cordova-plugin-splashscreen",
    "cordova-plugin-statusbar",
    "cordova-plugin-whitelist",
    "ionic-plugin-keyboard",
    "https://github.com/zfoltin/cordova-plugin-app-event.git",
    "cordova-plugin-inappbrowser",
    "cordova-plugin-spinner-dialog",
    "https://github.com/katzer/cordova-plugin-local-notifications.git#0.8.5",
    "https://github.com/OpenNMS/cordova-plugin-json-storage.git",
    "https://github.com/RangerRick/cordova-certificate-plugin.git",
    "https://github.com/RangerRick/cordova-HTTP.git",
    "https://github.com/Red-Folder/bgs-core.git",
    "https://github.com/VersoSolutions/CordovaClipboard.git",
    "cordova-plugin-datepicker",
    "cordova-plugin-filepath",
    "cordova-plugin-chrome-apps-system-memory",
    "https://github.com/RangerRick/photoviewer.git#master",
    "https://github.com/christocracy/cordova-plugin-background-fetch.git#4.0.4",
    "https://github.com/RangerRick/cordova-plugin-simple-image-picker.git",
    "https://github.com/EddyVerbruggen/Toast-PhoneGap-Plugin.git",
    "cordova-plugin-camera"
  ],
  "cordovaPlatforms": [
    {
      "platform": "ios",
      "version": "4.3.1",
      "locator": "ios"
    },
    {
      "platform": "android",
      "version": "6.2.3",
      "locator": "android"
    }
  ],
  "config": {
    "ghooks": {
      "pre-push": "npm run test",
      "post-merge": "npm install",
      "post-rewrite": "npm install"
    }
  },
  "scripts": {
    "setup": "bower install && npm install && ionic state reset",
    "pack": "webpack --colors --display-error-details",
    "pack-release": "npm run pack -- --env=production",
    "build": "npm run pack && ionic build",
    "prepare": "npm run pack && ionic prepare",
    "increment": "bin/update-build-number.js && npm run prepare",
    "release": "npm run lint && npm run test && npm run pack-release && ionic build --release --buildConfig build.json ios android",
    "verbose-build": "npm run lint && npm run test && npm run pack -- --display-modules --display-chunks --display-error-details --display-origins --display-reasons",
    "genymotion": "ionic run -slc android",
    "android": "ionic emulate -slc android",
    "ios": "ionic emulate -slc --address=localhost ios",
    "device": "ionic run --device -slc ios",
    "watch": "webpack --colors --display-error-details --watch",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -u",
    "lint": "eslint . --ext .js",
    "test": "mocha test"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-camera": {
        "CAMERA_USAGE_DESCRIPTION": "Used to take photos to post to Twit-arr.",
        "PHOTOLIBRARY_USAGE_DESCRIPTION": "Used to select photos to post to Twit-arr."
      },
      "cordova-plugin-statusbar": {},
      "cordova-plugin-chrome-apps-system-memory": {},
      "cordova-plugin-file-transfer": {},
      "cordova-plugin-datepicker": {},
      "cordova-plugin-splashscreen": {},
      "cordova-plugin-console": {},
      "cordova-plugin-file": {},
      "cordova-plugin-spinner-dialog": {},
      "ionic-plugin-keyboard": {},
      "cordova-plugin-device": {},
      "cordova-plugin-filepath": {},
      "cordova-plugin-dialogs": {},
      "cordova-plugin-network-information": {},
      "cordova-plugin-whitelist": {},
      "cordova-plugin-inappbrowser": {},
      "cordova-plugin-background-fetch": {},
      "de.appplant.cordova.plugin.local-notification": {},
      "cordova-plugin-app-event": {},
      "com.raccoonfink.cordova.plugins.http": {},
      "cordova-plugin-json-storage": {},
      "com.red_folder.phonegap.plugin.backgroundservice": {},
      "cordova-plugin-x-toast": {}
    },
    "platforms": [
      "android"
    ]
  }
}
