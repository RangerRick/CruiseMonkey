    "expose-loader": "^0.7.1",
    "file-loader": "^0.9.0",
    "fs.extra": "*",
    "ghooks": "^2.0.0",
    "html-loader": "^0.4.2",
    "imports-loader": "^0.7.0",
    "jsdom": "^9.9.1",
    "jshint": "*",
    "jshint-stylish": "*",
    "line-reader": "*",
    "lodash": "^4.5.0",
    "mocha": "^3.2.0",
    "moment": "^2.11.2",
    "moment-timezone": "^0.5.0",
    "ng-annotate-webpack-plugin": "^0.2.1-pre",
    "ngtemplate-loader": "^1.3.1",
