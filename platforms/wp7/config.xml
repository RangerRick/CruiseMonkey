<?xml version='1.0' encoding='utf-8'?>
<widget id="com.raccoonfink.cruisemonkey" version="3.9.2" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <feature name="File">
        <param name="wp-package" value="File" />
    </feature>
    <name>CruiseMonkey</name>
    <description>Keep Track of Your Cruise</description>
    <author email="ranger@befunk.com" href="https://github.com/RangerRick/CruiseMonkey.git">
		Benjamin Reed
	</author>
    <gap:platform minVersion="6.0" name="ios" />
    <gap:platform minVersion="4.0" name="android" />
    <gap:url-scheme name="com.raccoonfink.cruisemonkey" role="None">
        <scheme>cruisemonkey</scheme>
    </gap:url-scheme>
    <icon src="Icon.png" />
    <icon gap:platform="ios" height="29" src="Icon-Small.png" width="29" />
    <icon gap:platform="ios" height="50" src="Icon-Small-50.png" width="50" />
    <icon gap:platform="ios" height="57" src="Icon-57.png" width="57" />
    <icon gap:platform="ios" height="58" src="Icon-Small@2x.png" width="58" />
    <icon gap:platform="ios" height="72" src="Icon.png" width="72" />
    <icon gap:platform="ios" height="100" src="Icon-Small-50@2x.png" width="100" />
    <icon gap:platform="ios" height="114" src="Icon@2x.png" width="114" />
    <icon gap:platform="ios" height="144" src="Icon-72@2x.png" width="144" />
    <icon gap:platform="ios" height="320" src="Icon-320.png" width="320" />
    <icon gap:platform="android" height="36" src="Icon-36.png" width="36" />
    <icon gap:platform="android" height="48" src="Icon-48.png" width="48" />
    <icon gap:platform="android" height="72" src="Icon-72.png" width="72" />
    <icon gap:platform="android" height="96" src="Icon-96.png" width="96" />
    <gap:splash gap:platform="ios" height="1024" src="cruise-monkey-768x1024.png" width="768" />
    <gap:splash gap:platform="ios" height="2048" src="cruise-monkey-1536x2048.png" width="1536" />
    <gap:splash gap:platform="ios" height="480" src="cruise-monkey-320x480.png" width="320" />
    <gap:splash gap:platform="ios" height="960" src="cruise-monkey-640x960.png" width="640" />
    <gap:splash gap:platform="ios" height="1136" src="cruise-monkey-640x1136.png" width="640" />
    <gap:plugin name="org.apache.cordova.camera" />
    <gap:plugin name="org.apache.cordova.console" />
    <gap:plugin name="org.apache.cordova.device" />
    <gap:plugin name="org.apache.cordova.device-orientation" />
    <gap:plugin name="org.apache.cordova.network-information" />
    <gap:plugin name="org.apache.cordova.dialogs" />
    <gap:plugin name="org.apache.cordova.splashscreen" />
    <gap:plugin name="org.apache.cordova.vibration" />
    <gap:plugin name="org.apache.cordova.inappbrowser" />
    <preference name="phonegap-version" value="3.3.0" />
    <preference name="orientation" value="portrait" />
    <preference name="ios-statusbarstyle" value="black-opaque" />
    <preference name="detect-data-types" value="false" />
    <preference name="webviewbounce" value="false" />
    <preference name="android-minSdkVersion" value="10" />
    <preference name="android-targetSdkVersion" value="16" />
    <preference name="android-installLocation" value="auto" />
    <preference name="splash-screen-duration" value="20000" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="SplashScreen" value="splash" />
    <access origin="*" />
    <feature name="Camera">
        <param name="wp-package" value="Camera" />
    </feature>
    <feature name="Console">
        <param name="wp-package" value="DebugConsole" />
    </feature>
    <feature name="Device">
        <param name="wp-package" value="Device" />
    </feature>
    <feature name="Compass">
        <param name="wp-package" value="Compass" />
    </feature>
    <feature name="Notification">
        <param name="wp-package" value="Notification" />
    </feature>
    <feature name="Media">
        <param name="wp-package" value="Media" />
    </feature>
    <feature name="Capture">
        <param name="wp-package" value="Capture" />
    </feature>
    <feature name="NetworkStatus">
        <param name="wp-package" value="NetworkStatus" />
    </feature>
    <feature name="SplashScreen">
        <param name="wp-package" value="SplashScreen" />
    </feature>
    <feature name="Vibration">
        <param name="wp-package" value="Vibration" />
    </feature>
    <feature name="InAppBrowser">
        <param name="wp-package" value="InAppBrowser" />
    </feature>
</widget>
